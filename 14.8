    try:
        with open ("content.txt", "r") as filelist:
            allnames = []
            line = filelist.readline().split()
            while line:
                allnames.extend(line)
                line = filelist.readline().split()
    except FileNotFoundError:
        print('File not found bleat')
    for nam3 in allnames:
