def sumfile(file):
    try:
        f = open(file, "r")
        numbers = f.read().split()
        numbers = [int(_) for _ in numbers]
        return print(sum(numbers))
    except FileNotFoundError:
        print('File not found')
    except ValueError:
        print('Incorrect input')


